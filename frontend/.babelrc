{
  "presets": [
    "react-app"
  ],
  "plugins": [
    ["babel-plugin-react-css-modules", {
      "generateScopedName":  "[name]__[local]___[hash:base64:5]",
      "filetypes": {
        ".scss": {
          "syntax": "postcss-scss"
        }
      },
      "handleMissingStyleName": "warn"
    }]
  ]
}